          SuperscriptBox["\[PartialD]", "2"], 
          SubscriptBox["\[Pi]", "j"]}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["R", "2"]}]]}]}]}], "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "a", "b"], 
      RowBox[{
       RowBox[{"\[DifferentialD]", "R"}], " ", 
       SubscriptBox["\[Pi]", "i"], 
       RowBox[{
        SubscriptBox["V", "eff"], "(", "R", ")"}], 
       SubscriptBox["\[Pi]", "j"]}]}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
