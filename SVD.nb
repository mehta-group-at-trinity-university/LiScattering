 FormBox[
  RowBox[{
   RowBox[{
    TemplateBox[{SubscriptBox["\[Pi]", "i"]},
     "Bra"], "H", 
    TemplateBox[{SubscriptBox["\[Pi]", "j"]},
     "Ket"]}], "=", 
   RowBox[{
    RowBox[{
     TemplateBox[{SubscriptBox["\[Pi]", "i"]},
      "Bra"], "-", 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[HBar]", "2"], 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{
       FractionBox[
        SuperscriptBox["\[PartialD]", "2"], 
