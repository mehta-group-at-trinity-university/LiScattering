       RowBox[{"+", 
        RowBox[{
         SubscriptBox["V", "eff"], "(", "R", ")"}]}]}], 
      TemplateBox[{SubscriptBox["\[Pi]", "j"]},
       "Ket"]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        SuperscriptBox["\[HBar]", "2"], 
